            <if test="recoverstaffdate != null">recoverStaffDate = #{recoverstaffdate},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCmfHrmsFileById" parameterType="Integer">
        delete from cmf_hrms_file where id = #{id}
