            <if test="creator != null and creator != ''">#{creator},</if>
            <if test="creattime != null">#{creattime},</if>
         </trim>
    </insert>

    <update id="updateCmfHrmsTransfer" parameterType="CmfHrmsTransfer">
        update cmf_hrms_transfer
        <trim prefix="SET" suffixOverrides=",">
