            <if test="transfertype != null and transfertype != ''">transferType,</if>
            <if test="creator != null and creator != ''">creator,</if>
            <if test="creattime != null">creatTime,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
