            <if test="telNo != null and telNo != ''">TEL_NO,</if>
            <if test="faxNo != null and faxNo != ''">FAX_NO,</if>
            <if test="postNo != null and postNo != ''">POST_NO,</if>
            <if test="address != null and address != ''">ADDRESS,</if>
