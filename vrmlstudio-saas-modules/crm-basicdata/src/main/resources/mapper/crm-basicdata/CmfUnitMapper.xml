            <if test="telNo != null and telNo != ''">TEL_NO = #{telNo},</if>
            <if test="faxNo != null and faxNo != ''">FAX_NO = #{faxNo},</if>
            <if test="postNo != null and postNo != ''">POST_NO = #{postNo},</if>
